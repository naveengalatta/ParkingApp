<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminOrderDetails">

    <TextView
        android:id="@+id/apo_booking_id_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="25dp"
        android:text="Booking ID -"
        android:textSize="20sp"
        android:textColor="@android:color/black"
        android:padding="2dp">
    </TextView>

    <TextView
        android:id="@+id/apo_booking_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="15dp"
        android:layout_marginTop="25dp"
        android:layout_toRightOf="@+id/apo_booking_id_title"
        android:hint="Booking Id"
        android:textSize="20sp"
        android:padding="2dp">
    </TextView>

    <TextView
        android:id="@+id/apo_user_name_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_below="@+id/apo_booking_id"
        android:text="Name -"
        android:textSize="20sp"
        android:textColor="@android:color/black"
        android:padding="2dp">
    </TextView>

    <TextView
        android:id="@+id/apo_user_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="15dp"
        android:layout_below="@+id/apo_booking_id"
        android:layout_toRightOf="@+id/apo_user_name_title"
        android:hint="User Name"
        android:textSize="20sp"
        android:padding="2dp">
    </TextView>

    <TextView
        android:id="@+id/apo_user_phone_number_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_below="@+id/apo_user_name"
        android:text="Phone Number -"
        android:textSize="20sp"
        android:textColor="@android:color/black"
        android:padding="2dp">
    </TextView>

    <TextView
        android:id="@+id/apo_user_phone_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="15dp"
        android:layout_below="@+id/apo_user_name"
        android:layout_toRightOf="@+id/apo_user_phone_number_title"
        android:hint="User Phone Number"
        android:textSize="20sp"
        android:padding="2dp">
    </TextView>

    <TextView
        android:id="@+id/apo_vehicle_number_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_below="@+id/apo_user_phone_number"
        android:text="Vehicle Number -"
        android:textSize="20sp"
        android:padding="2dp"
        android:textColor="@android:color/black">
    </TextView>

    <TextView
        android:id="@+id/apo_vehicle_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="15dp"
        android:layout_below="@+id/apo_user_phone_number"
        android:layout_toRightOf="@+id/apo_vehicle_number_title"
        android:hint="Vehicle Number"
        android:textSize="20sp"
        android:padding="2dp">
    </TextView>

    <TextView
        android:id="@+id/apo_category_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_below="@+id/apo_vehicle_number"
        android:text="Category -"
        android:textSize="20sp"
        android:textColor="@android:color/black"
        android:padding="2dp">
    </TextView>

    <TextView
        android:id="@+id/apo_category"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="15dp"
        android:layout_below="@+id/apo_vehicle_number"
        android:layout_toRightOf="@+id/apo_category_title"
        android:hint="Category"
        android:textSize="20sp">
    </TextView>

    <TextView
        android:id="@+id/apo_date_of_booking_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_below="@+id/apo_category"
        android:text="Booking Date -"
        android:textSize="20sp"
        android:padding="2dp"
        android:textColor="@android:color/black">
    </TextView>

    <TextView
        android:id="@+id/apo_date_of_booking"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="15dp"
        android:layout_below="@+id/apo_category"
        android:layout_toRightOf="@+id/apo_date_of_booking_title"
        android:hint="Booking Date"
        android:textSize="20sp"
        android:padding="2dp">
    </TextView>

    <TextView
        android:id="@+id/apo_total_hours_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_below="@+id/apo_date_of_booking"
        android:text="Total Hours -"
        android:textSize="20sp"
        android:textColor="@android:color/black">
    </TextView>

    <TextView
        android:id="@+id/apo_total_hours"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="15dp"
        android:layout_below="@+id/apo_date_of_booking"
        android:layout_toRightOf="@+id/apo_total_hours_title"
        android:hint="Total Hours"
        android:textSize="20sp"
        android:padding="2dp">
    </TextView>

    <TextView
        android:id="@+id/apo_total_charges_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_below="@+id/apo_total_hours"
        android:text="Total Amount -"
        android:textSize="20sp"
        android:padding="2dp"
        android:textColor="@android:color/black">
    </TextView>

    <TextView
        android:id="@+id/apo_total_charges"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="15dp"
        android:layout_below="@+id/apo_total_hours"
        android:layout_toRightOf="@+id/apo_total_charges_title"
        android:hint="Total Amount"
        android:textSize="20sp"
        android:padding="2dp">
    </TextView>

</RelativeLayout>